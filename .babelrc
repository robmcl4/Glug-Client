{
  "presets": [ [ "es2015", {"loose": true, "modules": false}], "stage-0", "react" ],
  "env": {
    "production": {
      "plugins": [
        "transform-react-remove-prop-types",
        "transform-decorators-legacy"
      ]
    },
    "development": {
      "plugins": [
        "react-hot-loader/babel",
        "transform-decorators-legacy"
      ]
    }
  }
}
